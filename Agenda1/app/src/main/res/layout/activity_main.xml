<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.margarita.agenda1.MainActivity">

    <TableLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </TableLayout>

    <EditText
        android:id="@+id/nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nombre"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/apellidos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Apelllidos"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="72dp" />

    <EditText
        android:id="@+id/direccion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Direccion"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="132dp" />

    <EditText
        android:id="@+id/correoe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="CorreoE"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="178dp" />

    <EditText
        android:id="@+id/telefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Telefono"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="232dp" />

    <Button
        android:id="@+id/btnalta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="alta"
        android:text="Alta"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="315dp" />

    <Button
        android:id="@+id/btnbaja"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="baja"
        android:text="Baja"
        tools:layout_editor_absoluteX="253dp"
        tools:layout_editor_absoluteY="315dp" />

    <Button
        android:id="@+id/btnconsulta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="consulta"
        android:text="ConsultarDatos"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="394dp" />

    <Button
        android:id="@+id/btnmodificacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="modificar"
        android:text="Modificaciones"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="394dp" />
</android.support.constraint.ConstraintLayout>
